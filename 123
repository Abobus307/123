local Players = game:GetService("Players")
local player = Players.LocalPlayer

--------------------------------------------------
-- Ğ¡Ğ¾Ğ·Ğ´Ğ°Ñ‘Ğ¼ ĞµĞ´Ğ¸Ğ½Ñ‹Ğ¹ ScreenGui Ğ´Ğ»Ñ Ğ²ÑĞµÑ… UI
--------------------------------------------------
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player:WaitForChild("PlayerGui")
screenGui.Name = "SpeedAndJumpGUI"

--------------------------------------------------
-- GUI Ğ´Ğ»Ñ Ğ¸Ğ·Ğ¼ĞµĞ½ĞµĞ½Ğ¸Ñ ÑĞºĞ¾Ñ€Ğ¾ÑÑ‚Ğ¸ Ğ¸ Ğ¿Ñ€Ñ‹Ğ¶ĞºĞ°
--------------------------------------------------
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 150)
frame.Position = UDim2.new(0.5, -100, 0.5, -75)
frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
frame.Parent = screenGui
frame.Visible = false

-- Ğ’Ğ¾Ğ·Ğ¼Ğ¾Ğ¶Ğ½Ğ¾ÑÑ‚ÑŒ Ğ¿ĞµÑ€ĞµĞ¼ĞµÑ‰ĞµĞ½Ğ¸Ñ Ğ¾ĞºĞ½Ğ°
local dragging = false
local dragInput, dragStart, startPos

frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = frame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

frame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

local userInputService = game:GetService("UserInputService")
userInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)

-- ĞšĞ½Ğ¾Ğ¿ĞºĞ° Ğ¾Ñ‚ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ Ğ¿Ğ°Ñ€Ğ°Ğ¼ĞµÑ‚Ñ€Ğ¾Ğ² (Ğ¸Ğ·Ğ½Ğ°Ñ‡Ğ°Ğ»ÑŒĞ½Ğ¾ ÑĞºÑ€Ñ‹Ñ‚Ğ° Ğ´Ğ¾ Ğ²Ğ²Ğ¾Ğ´Ğ° ĞºĞ»ÑÑ‡Ğ°)
local openButton = Instance.new("TextButton")
openButton.Size = UDim2.new(0, 150, 0, 50)
openButton.Position = UDim2.new(1, -160, 0.5, -25)
openButton.AnchorPoint = Vector2.new(1, 0.5)
openButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
openButton.TextColor3 = Color3.fromRGB(255, 255, 255)
openButton.Font = Enum.Font.SourceSans
openButton.TextSize = 16
openButton.Text = "ĞÑ‚ĞºÑ€Ñ‹Ñ‚ÑŒ Ğ¿Ğ°Ñ€Ğ°Ğ¼ĞµÑ‚Ñ€Ñ‹"
openButton.Parent = screenGui
openButton.Visible = false  -- Ğ¿Ğ¾ĞºĞ°Ğ·Ñ‹Ğ²Ğ°ĞµÑ‚ÑÑ Ñ‚Ğ¾Ğ»ÑŒĞºĞ¾ Ğ¿Ğ¾ÑĞ»Ğµ Ğ²Ğ²Ğ¾Ğ´Ğ° Ğ¿Ñ€Ğ°Ğ²Ğ¸Ğ»ÑŒĞ½Ğ¾Ğ³Ğ¾ ĞºĞ»ÑÑ‡Ğ°

-- ĞšĞ½Ğ¾Ğ¿ĞºĞ° Ğ·Ğ°ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ Ğ¾ĞºĞ½Ğ° Ğ¿Ğ°Ñ€Ğ°Ğ¼ĞµÑ‚Ñ€Ğ¾Ğ²
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0, 30, 0, 30)
closeButton.Position = UDim2.new(1, -1, 0, -25)
closeButton.AnchorPoint = Vector2.new(1, 0)
closeButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.Font = Enum.Font.SourceSans
closeButton.TextSize = 22
closeButton.Text = "X"
closeButton.Parent = frame

-- Ğ—Ğ°Ğ³Ğ¾Ğ»Ğ¾Ğ²Ğ¾Ğº Ğ¾ĞºĞ½Ğ° Ğ¿Ğ°Ñ€Ğ°Ğ¼ĞµÑ‚Ñ€Ğ¾Ğ²
local speedJumpTitle = Instance.new("TextLabel")
speedJumpTitle.Size = UDim2.new(1, 0, 0, 30)
speedJumpTitle.Position = UDim2.new(0, 0, 0, 0)
speedJumpTitle.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
speedJumpTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
speedJumpTitle.Font = Enum.Font.SourceSansBold
speedJumpTitle.Text = "ĞĞ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸"
speedJumpTitle.TextSize = 18
speedJumpTitle.Parent = frame

-- ĞŸĞ¾Ğ»Ğµ Ğ´Ğ»Ñ Ğ²Ğ²Ğ¾Ğ´Ğ° ÑĞºĞ¾Ñ€Ğ¾ÑÑ‚Ğ¸
local speedLabel = Instance.new("TextLabel")
speedLabel.Size = UDim2.new(1, 0, 0, 20)
speedLabel.Position = UDim2.new(0, 0, 0, 40)
speedLabel.BackgroundTransparency = 1
speedLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
speedLabel.Font = Enum.Font.SourceSans
speedLabel.TextSize = 16
speedLabel.Text = "Ğ¡ĞºĞ¾Ñ€Ğ¾ÑÑ‚ÑŒ"
speedLabel.Parent = frame

local speedBox = Instance.new("TextBox")
speedBox.Size = UDim2.new(1, -10, 0, 20)
speedBox.Position = UDim2.new(0, 5, 0, 65)
speedBox.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
speedBox.TextColor3 = Color3.fromRGB(255, 255, 255)
speedBox.Font = Enum.Font.SourceSans
speedBox.PlaceholderText = "Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ÑĞºĞ¾Ñ€Ğ¾ÑÑ‚ÑŒ (Ğ¼Ğ°ĞºÑ 100)"
speedBox.Text = ""
