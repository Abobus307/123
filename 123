	else
		warn("Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ setclipboard Ğ½ĞµĞ´Ğ¾ÑÑ‚ÑƒĞ¿Ğ½Ğ°")
	end
end)

-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸ ĞºĞ»ÑÑ‡Ğ°
local function checkCode()
	if inputBox.Text == "Vasia2Sigma" then
		keyFrame.Visible = false
		openButton.Visible = true
	else
		inputBox.Text = "âŒ ĞĞµĞ²ĞµÑ€Ğ½Ñ‹Ğ¹ ĞºĞ»ÑÑ‡!"
		inputBox.TextColor3 = Color3.fromRGB(255, 0, 0)
		wait(1)
		inputBox.Text = ""
		inputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
	end
end

enterButtonKey.MouseButton1Click:Connect(checkCode)
