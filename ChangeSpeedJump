   Duration = 6.5,
   Image = 4483362458,
})

local Window = Rayfield:CreateWindow({
   Name = "Change jump and speed Script by Вася2",
   Icon = 0,
   LoadingTitle = "Jump And Speed",
   LoadingSubtitle = "By Вася2",
   Theme = "Default",
